{
  "name": "fragforce.org",
  "description": "Fragforce Dev PR",
  "scripts": {
    "postdeploy": "python manage.py migrate"
  },
  "env": {
    "DEBUG": {
      "required": true
    },
    "EXTRALIFE_TEAMID": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true,
      "description": "A secret key used by Django for various tasks",
      "generator": "secret"
    }
  },
  "formation": {},
  "addons": [
    {
      "plan": "heroku-postgresql:standard-0",
      "as": "DB",
      "options": {
      }
    },
    {
      "plan": "heroku-redis:premium-0",
      "as": "FILE",
      "options": {
        "maxmemory-policy":"allkeys-lru"
      }
    },
    {
      "plan": "heroku-redis:premium-0",
      "as": "CACHE",
      "options": {
        "maxmemory-policy":"allkeys-lru"
      }
    },
    {
      "plan": "herokuconnect:danketsu",
      "options": {
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "environments": {
    "test": {
      "scripts": {
        "test": "python manage.py test"
      }
    }
  }
}
